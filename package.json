    "@rollup/plugin-url": "^7.0.0",
    "@rollup/plugin-wasm": "^5.2.0",
    "@types/jest": "^24.0.16",
    "jasmine-core": "^4.1.1",
    "jest": "^28.1.0",
    "karma": "^4.4.1",
		"karma-chrome-launcher": "^3.1.0",
		"karma-jasmine": "^3.1.1",
		"karma-mocha-reporter": "^2.2.3",
    "karma-rollup-preprocessor": "^7.0.8",
    "mocha": "^10.0.0",
    "npm-run-all": "^4.1.5",
